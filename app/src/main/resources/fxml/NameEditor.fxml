<?import com.easymobo.openlabeler.ui.NameEditor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root type="com.easymobo.openlabeler.ui.NameEditor" xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml" stylesheets="@/css/OpenLabeler.css">
    <children>
        <AnchorPane>
            <children>
                <TextField fx:id="textField" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                <Button fx:id="button" graphicTextGap="0.0" minHeight="10.0" minWidth="12.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="22.0" text="X" textFill="black" AnchorPane.bottomAnchor="4.0" AnchorPane.rightAnchor="4.0" AnchorPane.topAnchor="4.0">
                    <font>
                        <Font size="10.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
        <ListView fx:id="list" prefHeight="200.0" prefWidth="200.0"/>
    </children>
</fx:root>
